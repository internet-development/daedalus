{
  "name": "@internet-dev/daedalus",
  "version": "0.1.1",
  "description": "AI planning CLI for agentic coding orchestration",
  "type": "module",
  "author": "Caidan Williams <caidan@internet.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/internet-development/daedalus.git"
  },
  "homepage": "https://github.com/internet-development/daedalus#readme",
  "bugs": {
    "url": "https://github.com/internet-development/daedalus/issues"
  },
  "keywords": [
    "ai",
    "agent",
    "orchestration",
    "coding",
    "cli",
    "planning",
    "beans"
  ],
  "bin": {
    "daedalus": "dist/cli/index.js",
    "talos": "dist/cli/talos.js"
  },
  "files": [
    "dist/",
    "!dist/**/*.test.*",
    "!dist/test-utils/",
    "README.md",
    "LICENSE",
    "docs/",
    "skills/"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "tsx src/cli/index.ts",
    "build": "tsc",
    "start": "node dist/cli/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "prepublishOnly": "npm run typecheck && npm run build && npm test"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.23",
    "@ai-sdk/openai": "^3.0.19",
    "ai": "^6.0.50",
    "bash-tool": "^1.3.9",
    "chokidar": "^4.0.0",
    "commander": "^14.0.2",
    "nanoid": "^5.1.6",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "yaml": "^2.7.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/pino-pretty": "^4.7.5",
    "@vitest/coverage-v8": "1.6.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "1.6.0"
  }
}
